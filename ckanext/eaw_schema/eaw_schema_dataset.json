      "validators": "eaw_schema_publicationlink",
      "form_placeholder": "http://doi.org/10.1016/j.envsoft.2017.03.002",
      "field_name": "publicationlink",
      "form_attrs": {
        "data-module": "eaw_schema_checkpublication"
      }
    },
    {
      "default_value": true,
